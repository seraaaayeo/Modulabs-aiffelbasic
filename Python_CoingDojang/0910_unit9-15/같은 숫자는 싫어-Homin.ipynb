    "def solution(arr):\n",
    "    answer = [arr[0]]\n",
    "    \n",
    "    for arr_i in arr[1:]:\n",
    "        if answer[-1] != arr_i:\n",
    "            answer.append(arr_i)\n",
    "    return answer"
